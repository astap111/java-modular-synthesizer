    <Label layoutX="13.0" layoutY="184.0" text="OSCILLATOR 2" textFill="WHITE">
        <font>
            <Font name="Calibri" size="13.0"/>
        </font>
    </Label>
    <Label layoutX="34.0" layoutY="273.0" text="NOISE" textFill="WHITE">
        <font>
            <Font name="Calibri" size="13.0"/>
        </font>
    </Label>
    <Knob layoutX="35.0" layoutY="48.0" radius="30" min="0" max="150" value="100" fx:id="oscillator1Volume"/>
    <Knob layoutX="35.0" layoutY="138.0" radius="30" min="0" max="150" value="50" fx:id="oscillator2Volume"/>
    <Knob layoutX="35.0" layoutY="228.0" radius="30" min="0" max="150" value="10" fx:id="noiseVolume"/>
</Pane>
