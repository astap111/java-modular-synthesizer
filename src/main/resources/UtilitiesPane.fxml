        <font>
            <Font name="Calibri Bold" size="13.0"/>
        </font>
    </Label>

    <Label layoutX="22.0" layoutY="94.0" text="HIGH PASS" textFill="WHITE">
        <font>
            <Font name="Calibri" size="13.0"/>
        </font>
    </Label>
    <Label layoutX="14.0" layoutY="229.0" text="ATTENUATOR" textFill="WHITE">
        <font>
            <Font name="Calibri" size="13.0"/>
        </font>
    </Label>
    <Knob fx:id="attenuator" layoutX="35.0" layoutY="183.0" radius="30"/>
